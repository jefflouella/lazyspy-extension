{
  "name": "lazy-spy",
  "version": "1.0.0",
  "description": "Professional image loading analyzer Chrome extension",
  "main": "background.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:prod": "node build-production.js && npm run build",
    "clean": "rm -rf dist/*",
    "package": "npm run build:prod && zip -r lazy-spy-production.zip dist/ manifest.json popup.html popup.css content.css icons/ -x '*.map'",
    "test": "jest"
  },
  "dependencies": {
    "jspdf": "^2.5.1",
    "chart.js": "^4.4.1",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4",
    "jest": "^29.7.0"
  },
  "keywords": [
    "chrome-extension",
    "image-optimization",
    "lazy-loading",
    "performance",
    "web-development"
  ],
  "author": "Lazy Spy Team",
  "license": "MIT"
}
